 
 <%= form_with model: @membership do |form| %>
  <div>
   <%= form.label :name %><br>
   <%= form.text_field :name %>
   <% @membership.errors.full_messages_for(:name).each do |message| %>
   <div><%= message %></div>
   <% end %>
  </div>

  <div>
   <%= form.label :discription %><br>
   <%= form.text_area :discription %><br>
   <% @membership.errors.full_messages_for(:discription).each do |message| %>
   <div><%= message %></div>
   <% end %>
  </div>
     
  <div>
   <%#= form.label :images, style: "display: block" %>
   <%#= form.file_field :images %>
  </div>
    <div>
   <%= form.label :price %><br>
   <%= form.text_area :price%><br>
   <% @membership.errors.full_messages_for(:price).each do |message| %>
   <div><%= message %></div>
   <% end %>
  </div>
  
   <div>
   <%= form.label :days %><br>
   <%= form.text_area :days %><br>
   <% @membership.errors.full_messages_for(:days).each do |message| %>
   <div><%= message %></div>
   <% end %>
  </div>
  <div>
   <%= form.submit %>
  </div>
<% end %>
